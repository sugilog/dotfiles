[user]
  name = sugilog
  email = sugilog@gmail.com
[color]
  ui = auto
  diff = auto
  status = auto
  branch = auto
  interactive = auto
[alias]
  myclone = "!f(){ git clone --template=$HOME/dotfiles/git-template/ $@; }; f"
  co = checkout
  a = add
  s = status
  l = log
  c = commit
  done = tag -f 'latest' origin/master
  read = log -p --reverse 'latest'..origin/master
  info = remote -v
  subup = "!f(){ git submodule foreach 'git pull origin master'; git submodule update; }; f"
[core]
  editor = vim
[github]
  user = sugilog@gmail.com
[pager]
  log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
[diff]
  compactionHeuristic = true
